	  ***FeraLab***
-->
<!DOCTYPE MediaSettings [
<!ELEMENT MediaSettings (CamcorderProfiles,
                         EncoderOutputFileFormat+,
                         VideoEncoderCap+,
                         AudioEncoderCap+,
                         VideoDecoderCap,
                         AudioDecoderCap)>
<!ELEMENT CamcorderProfiles (EncoderProfile+, ImageEncoding+, ImageDecoding, Camera)>
<!ATTLIST CamcorderProfiles cameraId CDATA>
<!ELEMENT EncoderProfile (Video, Audio)>
<!ATTLIST EncoderProfile quality (high|low)>
<!ATTLIST EncoderProfile fileFormat (mp4|3gp)>
<!ATTLIST EncoderProfile duration (20|30|60)>
<!ELEMENT Video EMPTY>
<!ATTLIST Video codec (h264|h263|m4v)>
<!ATTLIST Video bitRate CDATA>
<!ATTLIST Video width CDATA>
<!ATTLIST Video height CDATA>
<!ATTLIST Video frameRate CDATA>
<!ELEMENT Audio EMPTY>
<!ATTLIST Audio codec (amrnb|amrwb|aac)>
<!ATTLIST Audio bitRate CDATA>
<!ATTLIST Audio sampleRate CDATA>
<!ATTLIST Audio channels (1|2)>
<!ELEMENT ImageEncoding EMPTY>
<!ATTLIST ImageEncoding quality (100|80|70)>
<!ELEMENT ImageDecoding EMPTY>
<!ATTLIST ImageDecoding memCap CDATA>
<!ELEMENT Camera EMPTY>
<!ATTLIST Camera previewFrameRate CDATA>
<!ELEMENT EncoderOutputFileFormat EMPTY>
<!ATTLIST EncoderOutputFileFormat name (mp4|3gp)>
<!ELEMENT VideoEncoderCap EMPTY>
<!ATTLIST VideoEncoderCap name (h264|h263|m4v)>
<!ATTLIST VideoEncoderCap enabled (true|false)>
<!ATTLIST VideoEncoderCap minBitRate CDATA>
<!ATTLIST VideoEncoderCap maxBitRate CDATA>
<!ATTLIST VideoEncoderCap minFrameWidth CDATA>
<!ATTLIST VideoEncoderCap maxFrameWidth CDATA>
<!ATTLIST VideoEncoderCap minFrameHeight CDATA>
<!ATTLIST VideoEncoderCap maxFrameHeight CDATA>
<!ATTLIST VideoEncoderCap minFrameRate CDATA>
<!ATTLIST VideoEncoderCap maxFrameRate CDATA>
<!ELEMENT AudioEncoderCap EMPTY>
<!ATTLIST AudioEncoderCap name (amrnb|amrwb|aac)>
<!ATTLIST AudioEncoderCap enabled (true|false)>
<!ATTLIST AudioEncoderCap minBitRate CDATA>
<!ATTLIST AudioEncoderCap maxBitRate CDATA>
<!ATTLIST AudioEncoderCap minSampleRate CDATA>
<!ATTLIST AudioEncoderCap maxSampleRate CDATA>
<!ATTLIST AudioEncoderCap minChannels (1|2)>
<!ATTLIST AudioEncoderCap maxChannels (1|2)>
]>

<MediaSettings>
    <CamcorderProfiles cameraId="0">
        <EncoderProfile quality="high" fileFormat="mp4" duration="60">
            <Video codec="m4v"
                   bitRate="8000000"
                   width="1280"
                   height="720"
                   frameRate="24" />
           <Audio codec="aac"
                   bitRate="128000"
                   sampleRate="44100"
                   channels="1" />
        </EncoderProfile>
        <EncoderProfile quality="low" fileFormat="3gp" duration="20">
            <Video codec="h263"
                   bitRate="96000"
                   width="320"
                   height="240"
                   frameRate="15" />
            <Audio codec="amrnb"
                   bitRate="12200"
                   sampleRate="8000"
                   channels="1" />
        </EncoderProfile>
        <ImageEncoding quality="100" />
        <ImageEncoding quality="80" />
        <ImageEncoding quality="70" />
        <ImageDecoding memCap="20000000" />
        <Camera previewFrameRate="0" />
    </CamcorderProfiles>
    <EncoderOutputFileFormat name="3gp" />
    <EncoderOutputFileFormat name="mp4" />
    <VideoEncoderCap name="h263" enabled="true"
        minBitRate="96000" maxBitRate="3000000"
        minFrameWidth="320" maxFrameWidth="800"
        minFrameHeight="240" maxFrameHeight="480"
        minFrameRate="1" maxFrameRate="30" />
    <VideoEncoderCap name="h264" enabled="true"
        minBitRate="96000" maxBitRate="3000000"
        minFrameWidth="320" maxFrameWidth="800"
        minFrameHeight="240" maxFrameHeight="480"
        minFrameRate="1" maxFrameRate="30" />
    <VideoEncoderCap name="m4v" enabled="true"
        minBitRate="96000" maxBitRate="8000000"
        minFrameWidth="320" maxFrameWidth="1280"
        minFrameHeight="240" maxFrameHeight="720"
        minFrameRate="1" maxFrameRate="24" />
    <AudioEncoderCap name="amrnb" enabled="true"
        minBitRate="5525" maxBitRate="12200"
        minSampleRate="8000" maxSampleRate="8000"
        minChannels="1" maxChannels="1" />
    <AudioEncoderCap name="aac" enabled="true"
        minBitRate="5525" maxBitRate="256000"
        minSampleRate="8000" maxSampleRate="48000"
        minChannels="1" maxChannels="2" />
</MediaSettings>

